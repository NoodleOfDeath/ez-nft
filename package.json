{
  "name": "eznft",
  "description": "Yet another library for generating NFT artwork, uploading NFT assets and metadata to IPFS, deploying NFT smart contracts, and minting NFT collections",
  "version": "0.0.8-2",
  "main": "dist/index.js",
  "export": "dist/index.js",
  "bin": {
    "eznft": "dist/eznft.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "bugs": {
    "url": "https://github.com/noodleofdeath/eznft/issues",
    "email": "github@noodleofdeath.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noodleofdeath/eznft.git"
  },
  "keywords": [
    "automation",
    "bitcoin",
    "blockchain",
    "cli",
    "command",
    "command-line",
    "console",
    "contract",
    "contracts",
    "crypto",
    "cryptocurrency",
    "ethereum",
    "nft",
    "polygon",
    "shell",
    "smart",
    "solidity",
    "terminal",
    "token",
    "tty"
  ],
  "homepage": "https://github.com/noodleofdeath/eznft",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "prepare": "npm run build",
    "test": "mocha --inline-diffs --timeout 1000000 tests/run.js",
    "generate-interface": "ts-node ./tools/response-to-interface.ts",
    "generate-docs": "typedoc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@pinata/sdk": "^1.1.25",
    "argparse": "^2.0.1",
    "bottleneck": "^2.19.5",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.5",
    "fs-extra": "^10.1.0",
    "gitly": "^2.1.6"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^14.18.16",
    "husky": "^1.3.1",
    "inquirer": "^1.1.2",
    "json-ts": "^1.6.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "ts-node": "^10.7.0",
    "tslint": "^5.16.0",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.6.3"
  }
}
